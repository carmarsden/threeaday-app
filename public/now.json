{
    "version": 2,
    "name": "threeaday-app",
    "alias": "carmarsden-threeaday-app",
    "routes": [
        {
          "src": "^/static/(.*)",
          "dest": "/static/$1"
        },
        {
          "src": ".*",
          "dest": "/index.html"
        }
    ],
    "env": {
        "REACT_APP_API_BASE_URL": "https://threeaday-server.herokuapp.com/api",
        "REACT_APP_TOKEN_KEY": "threeaday-client-auth-token"
    }
}